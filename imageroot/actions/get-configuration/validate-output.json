{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Get tailscale settings",
    "$id": "http://nethserver.org/json-schema/task/input/tailscale/get-configuration",
    "description": "Get tailscale settings",
    "examples": [
        {
            "host": "tailscale.domain.org",
            "enable_web_interface": false
        }
    ],
    "type": "object",
    "required": [
        "host",
        "enable_web_interface"
    ],
    "properties": {
        "host": {
            "type": "string",
            "description": "Host name for the application, like 'tailscale.domain.org'",
            "format": "idn-hostname"
        },
        "enable_web_interface": {
            "type": "boolean",
            "title": "Enable web interface",
            "description": "Enable web interface"
        }
    }
}
