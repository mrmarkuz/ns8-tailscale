#
# Read configuration
#

import os
import sys
import json
import agent

# Prepare return variable
config = {}

# Read current configuration from the environment file
config["host"] = os.getenv("TS_HOST","")
config["enable_web_interface"] = os.getenv("ENABLE_WEB_INTERFACE") == "True"

# Dump the configuration to stdout
json.dump(config, fp=sys.stdout)
